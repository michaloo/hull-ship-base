{
  "name": "hull-ship-base",
  "description": "A Hull connector/ship foudation",
  "version": "0.1.0",
  "homepage": "https://github.com/hull/hull-ship-base",
  "license": "MIT",
  "main": "lib",
  "author": {
    "name": "Hull",
    "email": "contact@hull.io",
    "url": "https://github.com/hull"
  },
  "engines": {
    "node": "6.x",
    "npm": "3.x"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/hull/hull-ship-base"
  },
  "bugs": {
    "url": "https://github.com/hull/hull-ship-base"
  },
  "scripts": {
    "build": "npm run clean && npm run build:server",
    "build:server": "./node_modules/.bin/babel src -d lib",
    "start": "node ./lib",
    "test": "npm run test:lint && npm run test:unit",
    "test:lint": "eslint src tests examples",
    "test:modules": "npm outdated --depth=0",
    "test:unit": "NODE_ENV=test nyc mocha --require babel-register -R spec ./tests",
    "clean": "rimraf lib",
    "update": "updtr",
    "documentation": "documentation build src/** -f html -o docs",
    "postinstall": "npm run build",
    "precommit": "npm run test:lint",
    "prepush": "npm test"
  },
  "keywords": [
    "hull",
    "ship",
    "hull-ship",
    "base",
    "foundation"
  ],
  "devDependencies": {
    "babel-eslint": "^7.1.1",
    "babel-watch": "^2.0.6",
    "chai": "^3.5.0",
    "documentation": "^4.0.0-beta.18",
    "eslint": "^3.14.1",
    "eslint-config-airbnb-base": "^11.0.1",
    "eslint-plugin-import": "^2.2.0",
    "husky": "^0.13.1",
    "mocha": "^3.2.0",
    "nyc": "^10.1.2",
    "rimraf": "^2.5.4",
    "updtr": "^0.2.3"
  },
  "dependencies": {
    "JSONStream": "^1.3.0",
    "babel-cli": "^6.22.2",
    "babel-core": "^6.22.1",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-stage-0": "^6.22.0",
    "babel-register": "^6.22.0",
    "basic-auth": "^1.1.0",
    "batch-stream": "^0.1.3",
    "bluebird": "^3.4.7",
    "body-parser": "^1.16.0",
    "connect-timeout": "^1.8.0",
    "csv-stream": "^0.1.3",
    "datadog-metrics": "^0.3.0",
    "dogapi": "^2.6.0",
    "ejs": "^2.5.5",
    "express": "^4.14.1",
    "hull": "^0.10.6",
    "kue": "^0.11.5",
    "kue-ui": "^0.1.0",
    "lodash": "^4.17.4",
    "minimist": "^1.2.0",
    "newrelic": "^1.36.2",
    "promise-streams": "^1.0.1",
    "raven": "^1.1.1",
    "request": "^2.79.0",
    "supply": "0.0.4",
    "urijs": "^1.18.5"
  }
}
